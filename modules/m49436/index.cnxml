          <m:mrow>
           <m:mo>−</m:mo><m:mn>1</m:mn>
          </m:mrow>
         </m:mtd>
         <m:mtd>
          <m:mn>1</m:mn>
         </m:mtd>
        </m:mtr>
        <m:mtr>
         <m:mtd>
          <m:mn>4</m:mn>
         </m:mtd>
         <m:mtd>
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd>
          <m:mn>1</m:mn>
         </m:mtd>
        </m:mtr>

       </m:mtable><m:mrow><m:mo>|</m:mo><m:mrow>
        <m:mtable>
         <m:mtr>
          <m:mtd>
           <m:mn>0</m:mn>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mn>3</m:mn>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mrow>
            <m:mn>4</m:mn>
           </m:mrow>
          </m:mtd>
         </m:mtr>

        </m:mtable>
       </m:mrow></m:mrow><m:mtable>
        <m:mtr>
         <m:mtd>
          <m:mn>2</m:mn>
         </m:mtd>
        </m:mtr>
        <m:mtr>
         <m:mtd>
          <m:mrow>
           <m:mo>−</m:mo><m:mn>1</m:mn>
          </m:mrow>
         </m:mtd>
        </m:mtr>
        <m:mtr>
         <m:mtd>
          <m:mn>0</m:mn>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>|</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mn>0</m:mn><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mo>−</m:mo><m:mn>1</m:mn>
       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
       <m:mn>1</m:mn>
      <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>
       <m:mn>1</m:mn>
      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
       <m:mn>4</m:mn>
      <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mn>1</m:mn><m:mrow><m:mo>(</m:mo>
       <m:mn>3</m:mn>
      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
       <m:mn>0</m:mn>
      <m:mo>)</m:mo></m:mrow><m:mo>−</m:mo><m:mn>4</m:mn><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mo>−</m:mo><m:mn>1</m:mn>
       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
       <m:mn>1</m:mn>
      <m:mo>)</m:mo></m:mrow><m:mo>−</m:mo><m:mn>0</m:mn><m:mrow><m:mo>(</m:mo>
       <m:mn>1</m:mn>
      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
       <m:mn>0</m:mn>
      <m:mo>)</m:mo></m:mrow><m:mo>−</m:mo><m:mn>1</m:mn><m:mrow><m:mo>(</m:mo>
       <m:mn>3</m:mn>
      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
       <m:mn>2</m:mn>
      <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mn>0</m:mn><m:mo>+</m:mo><m:mn>8</m:mn><m:mo>+</m:mo><m:mn>0</m:mn><m:mo>+</m:mo><m:mn>4</m:mn><m:mo>−</m:mo><m:mn>0</m:mn><m:mo>−</m:mo><m:mn>6</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mn>6</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</equation></solution>
</exercise></example>

