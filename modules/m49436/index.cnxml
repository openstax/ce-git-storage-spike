    <m:mtr>
     <m:mtd>
      <m:mn>400</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>715</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>4</m:mn>
     </m:mtd>
    </m:mtr>
   </m:mtable>

  </m:math>
  </para></problem>
  <solution id="fs-id1703903">
   <para id="fs-id1703905"> <m:math>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mfrac>
        <m:mn>1</m:mn>
        <m:mrow>
         <m:mn>100</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>,</m:mo><m:mn>0</m:mn>
      </m:mrow>
      <m:mo>)</m:mo></m:mrow>
    </m:mrow>
   </m:math>
   </para>
</solution></exercise>
  <exercise id="fs-id1527386"><problem id="fs-id1527387"><para id="fs-id1527388"> <m:math>
   <m:mtable columnalign="left">
    <m:mtr>
     <m:mtd>
      <m:mn>0.1</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>0.1</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>0.1</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>1.2</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
