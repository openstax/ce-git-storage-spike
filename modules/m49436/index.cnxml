   <m:mtd>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo>−</m:mo><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>6</m:mn>
   </m:mtd>
  </m:mtr>
