    </m:msub>

   </m:mrow>
   <m:mi>D</m:mi>
  </m:mfrac>
  <m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mrow><m:mo>|</m:mo> <m:mrow>
     <m:mtable>
      <m:mtr>
