   <m:mo stretchy="false">(</m:mo><m:mn>400</m:mn><m:mo>,</m:mo><m:mn>30</m:mn><m:mo>,</m:mo><m:mn>000</m:mn><m:mo stretchy="false">)</m:mo>
  </m:mrow>
 </m:math>
 </para>
</solution>
</exercise>
