</media>

<para id="fs-id1704961">The algebra is as follows:</para>
  <equation id="fs-id1704964" class="unnumbered"><label/> <m:math display="block">
 <m:mrow>
  <m:mrow><m:mo>|</m:mo> <m:mi>A</m:mi> <m:mo>|</m:mo></m:mrow><m:mo>=</m:mo><m:msub>
   <m:mi>a</m:mi>
   <m:mn>1</m:mn>
  </m:msub>
  <m:msub>
   <m:mi>b</m:mi>
   <m:mn>2</m:mn>
  </m:msub>
  <m:msub>
   <m:mi>c</m:mi>
   <m:mn>3</m:mn>
  </m:msub>
  <m:mo>+</m:mo><m:msub>
   <m:mi>b</m:mi>
   <m:mn>1</m:mn>
  </m:msub>
  <m:msub>
   <m:mi>c</m:mi>
   <m:mn>2</m:mn>
  </m:msub>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mn>3</m:mn>
  </m:msub>
  <m:mo>+</m:mo><m:msub>
   <m:mi>c</m:mi>
   <m:mn>1</m:mn>
  </m:msub>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mn>2</m:mn>
  </m:msub>
  <m:msub>
   <m:mi>b</m:mi>
   <m:mn>3</m:mn>
  </m:msub>
  <m:mo>−</m:mo><m:msub>
   <m:mi>a</m:mi>
   <m:mn>3</m:mn>
  </m:msub>
  <m:msub>
   <m:mi>b</m:mi>
