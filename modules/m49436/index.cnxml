  </m:msub>
  <m:msub>
   <m:mi>c</m:mi>
   <m:mn>1</m:mn>
  </m:msub>
  <m:mo>−</m:mo><m:msub>
   <m:mi>b</m:mi>
   <m:mn>3</m:mn>
  </m:msub>
  <m:msub>
   <m:mi>c</m:mi>
