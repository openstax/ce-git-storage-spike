     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</equation></problem>
  <solution id="fs-id1456559">
   <para id="fs-id1456561"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable columnalign="right">
