   <m:mi>D</m:mi>
   <m:mi>y</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mrow><m:mo>|</m:mo> <m:mrow>
   <m:mtable>
    <m:mtr>
     <m:mtd>
      <m:mn>1</m:mn>
     </m:mtd>
     <m:mtd>
      <m:mrow>
