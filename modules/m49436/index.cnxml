    <m:mtd columnalign="left">
     <m:mrow>
      <m:mrow><m:mo>|</m:mo> <m:mi>A</m:mi> <m:mo>|</m:mo></m:mrow><m:mo>=</m:mo><m:mrow><m:mo>|</m:mo> <m:mrow>
       <m:mtable>
        <m:mtr>
         <m:mtd>
          <m:mn>0</m:mn>
         </m:mtd>
