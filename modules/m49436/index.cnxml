       <m:mo>−</m:mo><m:mn>5</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>12</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable><m:mspace width="0.5em"/><m:mrow><m:mo>|</m:mo><m:mrow><m:mtable columnalign="right">
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mrow>
        <m:mn>140</m:mn>
       </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="right">
