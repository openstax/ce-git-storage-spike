       </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mn>0</m:mn>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mrow>
        <m:mn>−7</m:mn>
       </m:mrow>
      </m:mtd>
     </m:mtr>

    </m:mtable>
   </m:mrow></m:mrow>
  </m:mrow> <m:mo>]</m:mo></m:mrow>
 </m:mrow>
</m:math>
</para></solution></exercise>

