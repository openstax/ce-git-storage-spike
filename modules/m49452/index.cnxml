<exercise id="fs-id1165137935628">
<problem id="fs-id1165137935630">
<para id="fs-id1165135192366"> <m:math>
 <m:mrow>
  <m:munder>
   <m:mrow>
    <m:mi>lim</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mo>−</m:mo><m:msup>
     <m:mn>1</m:mn>
