    </m:msup>

   </m:mrow>
  </m:munder>
  <m:mspace width="0.8em"/><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165137600935">
<para id="fs-id1165137600937">2</para>
