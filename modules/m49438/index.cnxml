    <exercise id="fs-id1371116" class="material-set-2"><problem id="fs-id1371117"><para id="fs-id1371118"> <m:math>
     <m:mrow>
      <m:mn>4</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>24</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>36</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>360</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>864</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1342973"><para id="fs-id1342974"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mn>3</m:mn><m:msqrt>
              <m:mn>2</m:mn>
             </m:msqrt>

            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>y</m:mi><m:mo>−</m:mo><m:mn>5</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:msqrt>
              <m:mn>2</m:mn>
             </m:msqrt>

            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>;</m:mo>
      </m:mrow>
     </m:math> Endpoints of major axis <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>3</m:mn><m:mo>+</m:mo><m:mn>3</m:mn><m:msqrt>
          <m:mn>2</m:mn>
         </m:msqrt>
         <m:mo>,</m:mo><m:mn>5</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>3</m:mn><m:mo>−</m:mo><m:mn>3</m:mn><m:msqrt>
           <m:mn>2</m:mn>
          </m:msqrt>
          <m:mo>,</m:mo><m:mn>5</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo><m:mtext>  </m:mtext>
      </m:mrow>
     </m:math> Endpoints of minor axis <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>3</m:mn><m:mo>,</m:mo><m:mn>5</m:mn><m:mo>+</m:mo><m:msqrt>
          <m:mn>2</m:mn>
         </m:msqrt>

        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>3</m:mn><m:mo>,</m:mo><m:mn>5</m:mn><m:mo>−</m:mo><m:msqrt>
           <m:mn>2</m:mn>
          </m:msqrt>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo><m:mspace width="0.8"/><m:mtext> </m:mtext>
      </m:mrow>
     </m:math> Foci at <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>7</m:mn><m:mo>,</m:mo><m:mn>5</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mn>5</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
      </m:mrow>
     </m:math>
     </para></solution></exercise>
