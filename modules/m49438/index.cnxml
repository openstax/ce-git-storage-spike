    <exercise id="fs-id1349361"><problem id="fs-id1349362"><para id="fs-id1349363"> <m:math>
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mn>4</m:mn>
      </m:mfrac>
      <m:mo>+</m:mo><m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mi>y</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mn>9</m:mn>
      </m:mfrac>
      <m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1359219"><para id="fs-id1359220"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mn>2</m:mn>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>y</m:mi><m:mo>−</m:mo><m:mn>7</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mn>3</m:mn>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>;</m:mo>
      </m:mrow>
     </m:math> Endpoints of major axis <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>5</m:mn><m:mo>,</m:mo><m:mn>10</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>5</m:mn><m:mo>,</m:mo><m:mn>4</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
      </m:mrow>
     </m:math> Endpoints of minor axis <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>7</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>7</m:mn><m:mo>,</m:mo><m:mn>7</m:mn>
         </m:mrow>
