    <exercise id="fs-id1313180" class="material-set-2"><problem id="fs-id1313181"><para id="fs-id1313182"> <m:math>
     <m:mrow>
      <m:mn>4</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>24</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>25</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>200</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>336</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1332424"><para id="fs-id1332426"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mn>5</m:mn>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mn>2</m:mn>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>;</m:mo>
      </m:mrow>
     </m:math> Endpoints of major axis <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>2</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>4</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>8</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>4</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
      </m:mrow>
     </m:math> Endpoints of minor axis <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>2</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>6</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
      </m:mrow>
     </m:math> Foci at <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>+</m:mo><m:msqrt>
          <m:mrow>
           <m:mn>21</m:mn>
          </m:mrow>
         </m:msqrt>
         <m:mo>,</m:mo><m:mo>−</m:mo><m:mn>4</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>−</m:mo><m:msqrt>
           <m:mrow>
            <m:mn>21</m:mn>
           </m:mrow>
          </m:msqrt>
          <m:mo>,</m:mo><m:mo>−</m:mo><m:mn>4</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
      </m:mrow>
     </m:math> </para></solution></exercise>
