      <m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1385318"><para id="fs-id1385319"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mn>1</m:mn>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mi>y</m:mi>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mfrac>
              <m:mn>1</m:mn>
              <m:mn>3</m:mn>
             </m:mfrac>

            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>;</m:mo>
      </m:mrow>
     </m:math> Endpoints of major axis <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>1</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math> and <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
      </m:mrow>
     </m:math> Endpoints of minor axis <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>0</m:mn><m:mo>,</m:mo><m:mfrac>
          <m:mn>1</m:mn>
          <m:mn>3</m:mn>
         </m:mfrac>

        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>0</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
           <m:mn>1</m:mn>
           <m:mn>3</m:mn>
          </m:mfrac>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
      </m:mrow>
     </m:math> Foci at <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mfrac>
          <m:mrow>
           <m:mn>2</m:mn><m:msqrt>
            <m:mn>2</m:mn>
           </m:msqrt>

          </m:mrow>
          <m:mn>3</m:mn>
         </m:mfrac>
         <m:mo>,</m:mo><m:mn>0</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mfrac>
           <m:mrow>
            <m:mn>2</m:mn><m:msqrt>
             <m:mn>2</m:mn>
            </m:msqrt>

           </m:mrow>
           <m:mn>3</m:mn>
          </m:mfrac>
          <m:mo>,</m:mo><m:mn>0</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
      </m:mrow>
     </m:math>
     </para></solution></exercise>
