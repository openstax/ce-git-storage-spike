         <m:mo>−</m:mo><m:mn>5</m:mn><m:mo>+</m:mo><m:msqrt>
          <m:mrow>
           <m:mn>21</m:mn>
          </m:mrow>
         </m:msqrt>
         <m:mo>,</m:mo><m:mn>2</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>5</m:mn><m:mo>−</m:mo><m:msqrt>
           <m:mrow>
            <m:mn>21</m:mn>
           </m:mrow>
          </m:msqrt>
          <m:mo>,</m:mo><m:mn>2</m:mn>
