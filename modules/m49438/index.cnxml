    <exercise id="fs-id1332234"><problem id="fs-id1332235"><para id="fs-id1332236"> <m:math>
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mrow>
        <m:mn>25</m:mn>
       </m:mrow>
      </m:mfrac>
      <m:mo>+</m:mo><m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mrow>
        <m:mn>36</m:mn>
       </m:mrow>
      </m:mfrac>
      <m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1332042"><para id="fs-id1332043">Foci <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>1</m:mn><m:mo>+</m:mo><m:msqrt>
          <m:mrow>
           <m:mn>11</m:mn>
          </m:mrow>
         </m:msqrt>

        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:msqrt>
           <m:mrow>
            <m:mn>11</m:mn>
           </m:mrow>
          </m:msqrt>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math>
     </para></solution></exercise>
