     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1388767"><para id="fs-id1388768">Center <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>3</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Vertices <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>0</m:mn><m:mo>,</m:mo><m:mn>3</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>6</m:mn><m:mo>,</m:mo><m:mn>3</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
          </m:mrow>
          <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>6</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Focus <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>3</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math> </para>

    <para id="fs-id1327102">Note that this ellipse is a circle. The circle has only one focus, which coincides with the center.</para>

     <media id="fs-id1327108" alt="" display="block">
      <image mime-type="image/jpg" src="CNX_Precalc_Figure_10_01_206.jpg"/>
     </media>
</solution></exercise><exercise id="fs-id1354710"><problem id="fs-id1354711"><para id="fs-id1354712"> <m:math>
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mi>x</m:mi>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>+</m:mo><m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mn>5</m:mn>
      </m:mfrac>
      <m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:math>
    </para></problem></exercise>

    <exercise id="fs-id1332813"><problem id="fs-id1332814"><para id="fs-id1332815"> <m:math>
     <m:mrow>
      <m:mn>4</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>16</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>32</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>44</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1332871"><para id="fs-id1332872">Center <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>1</m:mn><m:mo>,</m:mo><m:mn>1</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Vertices <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>5</m:mn><m:mo>,</m:mo><m:mn>1</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>1</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mn>1</m:mn><m:mo>,</m:mo><m:mn>3</m:mn>
          </m:mrow>
          <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mn>1</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>1</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Foci <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>1</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo>+</m:mo><m:mn>4</m:mn><m:msqrt>
          <m:mn>3</m:mn>
         </m:msqrt>

        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>1</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mn>4</m:mn><m:msqrt>
           <m:mn>3</m:mn>
          </m:msqrt>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math>
     </para>

     <media id="fs-id1363950" alt="" display="block">
      <image mime-type="image/jpg" src="CNX_Precalc_Figure_10_01_208.jpg"/>
     </media>
</solution></exercise><exercise id="fs-id1363960"><problem id="fs-id1363961"><para id="fs-id1363962"> <m:math>
     <m:mrow>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>25</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>100</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>91</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para></problem></exercise>
    <exercise id="fs-id1326410"><problem id="fs-id1326411"><para id="fs-id1326412"> <m:math>
     <m:mrow>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>40</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>112</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1326466"><para id="fs-id1326468">Center <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>5</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Vertices <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mn>5</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>6</m:mn><m:mo>,</m:mo><m:mn>4</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>6</m:mn>
          </m:mrow>
          <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>4</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Foci <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>+</m:mo><m:msqrt>
          <m:mn>3</m:mn>
         </m:msqrt>
         <m:mo>,</m:mo><m:mn>5</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>−</m:mo><m:msqrt>
           <m:mn>3</m:mn>
          </m:msqrt>
          <m:mo>,</m:mo><m:mn>5</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math>
     </para>

     <media id="fs-id1344024" alt="" display="block">
      <image mime-type="image/jpg" src="CNX_Precalc_Figure_10_01_210.jpg"/>
     </media>
</solution></exercise><exercise id="fs-id1344033" class="material-set-2"><problem id="fs-id1344034"><para id="fs-id1344036"> <m:math>
     <m:mrow>
      <m:mn>64</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>128</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>9</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>72</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>368</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para></problem></exercise>
    <exercise id="fs-id1333303" class="material-set-2"><problem id="fs-id1333304"><para id="fs-id1333305"> <m:math>
     <m:mrow>
      <m:mn>16</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>64</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>8</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1194701"><para id="fs-id1194702">Center <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mn>1</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Vertices <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>0</m:mn><m:mo>,</m:mo><m:mn>1</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>1</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mn>5</m:mn>
          </m:mrow>
          <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>3</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Foci <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:msqrt>
          <m:mn>3</m:mn>
         </m:msqrt>

        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mn>2</m:mn><m:msqrt>
           <m:mn>3</m:mn>
          </m:msqrt>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math>
     </para>

     <media id="fs-id1396328" alt="" display="block">
      <image mime-type="image/jpg" src="CNX_Precalc_Figure_10_01_212.jpg"/>
     </media>
</solution></exercise><exercise id="fs-id1396338" class="material-set-2"><problem id="fs-id1396339"><para id="fs-id1396340"> <m:math>
     <m:mrow>
      <m:mn>100</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>1000</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>10</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>2425</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para></problem></exercise>
    <exercise id="fs-id1363717" class="material-set-2"><problem id="fs-id1363718"><para id="fs-id1363719"> <m:math>
     <m:mrow>
      <m:mn>4</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>16</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>16</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>16</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1326714"><para id="fs-id1326715">Center <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>2</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Vertices <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>0</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>2</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>2</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
          </m:mrow>
          <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>4</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Focus <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>2</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math>
     </para>

     <media id="fs-id1327616" alt="" display="block">
      <image mime-type="image/jpg" src="CNX_Precalc_Figure_10_01_214.jpg"/>
     </media>
</solution></exercise><para id="fs-id1327626">For the following exercises, use the given information about the graph of each ellipse to determine its equation.</para>

    <exercise id="fs-id1327630"><problem id="fs-id1327631"><para id="fs-id1327632">Center at the origin, symmetric with respect to the <emphasis effect="italics">x</emphasis>- and <emphasis effect="italics">y</emphasis>-axes, focus at <m:math>
 <m:mrow>
  <m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo>
 </m:mrow>
</m:math> and point on graph <m:math>
 <m:mrow>
  <m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo>
 </m:mrow>
</m:math> </para></problem></exercise>
    <exercise id="fs-id1304004"><problem id="fs-id1304005"><para id="fs-id1304006">Center at the origin, symmetric with respect to the <emphasis effect="italics">x</emphasis>- and <emphasis effect="italics">y</emphasis>-axes, focus at <m:math>
 <m:mrow>
  <m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mn>−2</m:mn><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo>
 </m:mrow>
</m:math> and point on graph <m:math>
 <m:mrow>
  <m:mo stretchy="false">(</m:mo><m:mn>5</m:mn><m:mo>,</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo>
 </m:mrow>
</m:math>
    </para></problem>
     <solution id="fs-id1368634"><para id="fs-id1368635"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:mn>25</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mi>y</m:mi>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:mn>29</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>=</m:mo><m:mn>1</m:mn>
      </m:mrow>
     </m:math>
     </para></solution></exercise>
    <exercise id="fs-id1368697"><problem id="fs-id1368698"><para id="fs-id1368699">Center at the origin, symmetric with respect to the <emphasis effect="italics">x</emphasis>- and <emphasis effect="italics">y</emphasis>-axes, focus at <m:math>
 <m:mrow>
  <m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo>
 </m:mrow>
</m:math> and major axis is twice as long as minor axis.</para></problem></exercise>
    <exercise id="fs-id1320506"><problem id="fs-id1320507"><para id="fs-id1320508">Center <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>4</m:mn><m:mo>,</m:mo><m:mn>2</m:mn>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math> ; vertex <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>9</m:mn><m:mo>,</m:mo><m:mn>2</m:mn>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math> ; one focus: <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>4</m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:msqrt>
         <m:mn>6</m:mn>
        </m:msqrt>
        <m:mo>,</m:mo><m:mn>2</m:mn>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math> .</para></problem>
     <solution id="fs-id1364313"><para id="fs-id1364314"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:mn>25</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>y</m:mi><m:mo>−</m:mo><m:mn>2</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mn>1</m:mn>
       </m:mfrac>
       <m:mo>=</m:mo><m:mn>1</m:mn>
      </m:mrow>
     </m:math>
     </para></solution></exercise>
    <exercise id="fs-id1303537"><problem id="fs-id1303538"><para id="fs-id1303539">Center <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>3</m:mn><m:mo>,</m:mo><m:mn>5</m:mn>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math> ; vertex <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>3</m:mn><m:mo>,</m:mo><m:mn>11</m:mn>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math> ; one focus: <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>3</m:mn><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext>5+4</m:mtext><m:msqrt>
         <m:mtext>2</m:mtext>
        </m:msqrt>

       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math>
    </para></problem></exercise>
    <exercise id="fs-id1346675"><problem id="fs-id1346676"><para id="fs-id1346678">Center <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
<m:mn>−3</m:mn><m:mo>,</m:mo><m:mn>4</m:mn>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math> ; vertex <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>1</m:mn><m:mo>,</m:mo><m:mn>4</m:mn>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math> ; one focus: <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
<m:mn>−3</m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>
        <m:mo>,</m:mo><m:mn>4</m:mn>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1396511"><para id="fs-id1396512"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:mn>16</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mrow>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:mi>y</m:mi><m:mo>−</m:mo><m:mn>4</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mn>4</m:mn>
       </m:mfrac>
       <m:mo>=</m:mo><m:mn>1</m:mn>
      </m:mrow>
     </m:math>
     </para></solution></exercise>

    <para id="fs-id1343632">For the following exercises, given the graph of the ellipse, determine its equation.</para>

