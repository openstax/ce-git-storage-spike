       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>49</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1404210"><para id="fs-id1404211">Center <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>0</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Vertices <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mfrac>
          <m:mn>1</m:mn>
          <m:mn>9</m:mn>
         </m:mfrac>
         <m:mo>,</m:mo><m:mn>0</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mfrac>
           <m:mn>1</m:mn>
           <m:mn>9</m:mn>
          </m:mfrac>
          <m:mo>,</m:mo><m:mn>0</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mn>0</m:mn><m:mo>,</m:mo><m:mfrac>
            <m:mn>1</m:mn>
            <m:mn>7</m:mn>
           </m:mfrac>

          </m:mrow>
          <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mn>0</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
             <m:mn>1</m:mn>
             <m:mn>7</m:mn>
            </m:mfrac>

           </m:mrow>
           <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mspace width="0.8"/><m:mtext> </m:mtext>
      </m:mrow>
     </m:math> Foci <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>0</m:mn><m:mo>,</m:mo><m:mfrac>
          <m:mrow>
           <m:mn>4</m:mn><m:msqrt>
            <m:mn>2</m:mn>
           </m:msqrt>

          </m:mrow>
          <m:mrow>
           <m:mn>63</m:mn>
          </m:mrow>
         </m:mfrac>

        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>0</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
           <m:mrow>
            <m:mn>4</m:mn><m:msqrt>
             <m:mn>2</m:mn>
            </m:msqrt>

           </m:mrow>
           <m:mrow>
            <m:mn>63</m:mn>
           </m:mrow>
          </m:mfrac>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math>
     </para><media id="fs-id1329173" alt="" display="block">
      <image mime-type="image/jpg" src="CNX_Precalc_Figure_10_01_204.jpg"/>
     </media>
</solution></exercise><exercise id="fs-id1329183"><problem id="fs-id1329184"><para id="fs-id1329185"> <m:math>
