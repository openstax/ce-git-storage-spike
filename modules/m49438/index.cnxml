       <m:mrow>
        <m:mn>2304</m:mn><m:mo>−</m:mo><m:mn>529</m:mn>
       </m:mrow>
      </m:msqrt>

     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
