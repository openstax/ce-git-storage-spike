          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn>
           </m:mrow>
           <m:mo>)</m:mo></m:mrow>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mn>9</m:mn>
      </m:mfrac>
      <m:mo>+</m:mo><m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
