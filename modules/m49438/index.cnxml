       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>100</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>100</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
