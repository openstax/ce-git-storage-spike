    <exercise id="fs-id1359968"><problem id="fs-id1359969"><para id="fs-id1359970"> <m:math>
     <m:mrow>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
