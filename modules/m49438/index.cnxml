    <exercise id="fs-id1331349"><problem id="fs-id1331350"><para id="fs-id1331351"> <m:math>
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mi>x</m:mi>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mrow>
        <m:mn>16</m:mn>
       </m:mrow>
      </m:mfrac>
      <m:mo>+</m:mo><m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mi>y</m:mi>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mn>9</m:mn>
      </m:mfrac>
      <m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1368186"><para id="fs-id1368187">Center <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>0</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Vertices <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>4</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo>
      </m:mrow>
     </m:math> Foci <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:msqrt>
          <m:mn>7</m:mn>
         </m:msqrt>
         <m:mo>,</m:mo><m:mn>0</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:msqrt>
           <m:mn>7</m:mn>
          </m:msqrt>
          <m:mo>,</m:mo><m:mn>0</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math>
     </para>

     <media id="fs-id1360065" alt="" display="block">
      <image mime-type="image/jpg" src="CNX_Precalc_Figure_10_01_202.jpg"/>
     </media>
</solution></exercise><exercise id="fs-id1360074"><problem id="fs-id1360075"><para id="fs-id1360076"> <m:math>
