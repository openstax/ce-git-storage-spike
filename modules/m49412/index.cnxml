   <m:mrow>
    <m:mn>5</m:mn><m:msqrt>
     <m:mn>3</m:mn>
    </m:msqrt>

   </m:mrow>
   <m:mn>2</m:mn>
  </m:mfrac>

 </m:mrow>
</m:math>
</para>
</solution></exercise>
