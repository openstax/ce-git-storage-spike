   <m:mrow>
    <m:mo>−</m:mo><m:mfrac>
     <m:mi>π</m:mi>
     <m:mn>3</m:mn>
    </m:mfrac>
    </m:mrow>
  <m:mo>)</m:mo></m:mrow></m:mrow>
</m:math>
</para>
</solution></exercise>
