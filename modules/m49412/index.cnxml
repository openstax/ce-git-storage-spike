<exercise id="fs-id1165135455023"><problem id="fs-id1165135455024"><para id="fs-id1165135455026"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>
        <m:mi>e</m:mi>
        <m:mi>t</m:mi>
       </m:msup>

      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
