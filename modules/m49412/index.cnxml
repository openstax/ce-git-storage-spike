   <m:mn>1</m:mn>
  </m:msub>
  <m:mo>=</m:mo><m:mn>2</m:mn><m:mi mathvariant="normal">cis</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mn>89°</m:mn></m:mrow>
  <m:mo>)</m:mo></m:mrow></m:mrow>
