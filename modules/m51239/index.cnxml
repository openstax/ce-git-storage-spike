  <m:mtd rowalign="center"/>
  <m:mtd rowalign="center"><m:mo>=</m:mo></m:mtd>
  <m:mtd columnalign="left" rowalign="center"><m:mrow><m:mn>−15</m:mn></m:mrow></m:mtd>
  </m:mtr>

  </m:mtable></m:mrow>
  </m:math>

  </equation></item>
  </list></solution>
  </exercise>
  </example><note id="fs-id2989790" class="precalculus try"><label>Try It</label>
