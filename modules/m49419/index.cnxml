        <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>8</m:mn>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mn>−4</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>4</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165134323586">
<para id="fs-id1165134323588"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>128</m:mn>
     </m:mrow>
     <m:mrow>
      <m:mn>557</m:mn>
     </m:mrow>
    </m:mfrac>
    <m:mo>,</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>23</m:mn>
     </m:mrow>
     <m:mrow>
      <m:mn>557</m:mn>
     </m:mrow>
    </m:mfrac>
    <m:mo>,</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>28</m:mn>
     </m:mrow>
     <m:mrow>
      <m:mn>557</m:mn>
     </m:mrow>
    </m:mfrac>

   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math>
</para>
</solution>
</exercise>
