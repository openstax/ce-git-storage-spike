        </m:mrow>
    </m:math>
    </para></problem>
    <solution id="fs-id2800207"><para id="fs-id2800208"> <m:math>
        <m:mrow>
            <m:mrow><m:mo>(</m:mo>
                <m:mrow>
                    <m:mn>−10</m:mn><m:mo>,</m:mo><m:mspace width="0.3em"/><m:mn>10</m:mn><m:msqrt>
                        <m:mn>3</m:mn>
                    </m:msqrt>

                </m:mrow>
                <m:mo>)</m:mo></m:mrow>
        </m:mrow>
    </m:math>
    </para></solution>
</exercise><exercise id="fs-id2761075">
