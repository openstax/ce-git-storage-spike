    <solution id="fs-id2757987"><para id="fs-id2757988"> <m:math>
        <m:mrow>
            <m:mrow><m:mo>(</m:mo>
                <m:mrow>
                    <m:mn>–2.778</m:mn><m:mo>,</m:mo><m:mtext> </m:mtext><m:mn>15.757</m:mn>
