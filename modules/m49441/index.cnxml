   <solution id="fs-id1264829">
    <para id="fs-id1264831"> <m:math>
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:msup>
             <m:mi>x</m:mi>
             <m:mo>′</m:mo>
            </m:msup>
            <m:mo>−</m:mo><m:msup>
             <m:mi>y</m:mi>
             <m:mo>′</m:mo>
            </m:msup>

           </m:mrow>
           <m:mo>)</m:mo></m:mrow>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mn>8</m:mn>
      </m:mfrac>
      <m:mo>+</m:mo><m:mfrac>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:msup>
             <m:mi>x</m:mi>
             <m:mo>′</m:mo>
            </m:msup>
            <m:mo>+</m:mo><m:msup>
             <m:mi>y</m:mi>
             <m:mo>′</m:mo>
            </m:msup>

           </m:mrow>
           <m:mo>)</m:mo></m:mrow>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:math>
    </para>

    <media id="fs-id1232420" alt="" display="block">
     <image mime-type="image/jpg" src="CNX_Precalc_Figure_10_04_203.jpg"/>
    </media>

</solution></exercise>
