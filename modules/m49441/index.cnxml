   <exercise id="fs-id2245359"><problem id="fs-id2245360"><para id="fs-id2245361"> <m:math>
    <m:mrow>
     <m:mi>x</m:mi><m:mo>=</m:mo><m:msup>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mi>y</m:mi><m:mo>−</m:mo><m:mn>1</m:mn>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow>
      </m:mrow>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>,</m:mo><m:mi>θ</m:mi><m:mo>=</m:mo><m:msup>
      <m:mrow>
       <m:mn>30</m:mn>
      </m:mrow>
      <m:mo>∘</m:mo>
     </m:msup>

    </m:mrow>
   </m:math>
   </para></problem>
   <solution id="fs-id2463275">
    <para id="fs-id2463278"> <m:math>
     <m:mrow>
      <m:mfrac>
       <m:mrow>
        <m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>

       </m:mrow>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mo>′</m:mo>
      </m:msup>
      <m:mo>−</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:msup>
       <m:mi>y</m:mi>
       <m:mo>′</m:mo>
      </m:msup>
      <m:mo>=</m:mo><m:msup>
       <m:mrow>
        <m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mfrac>
           <m:mn>1</m:mn>
           <m:mn>2</m:mn>
          </m:mfrac>
          <m:msup>
           <m:mi>x</m:mi>
           <m:mo>′</m:mo>
          </m:msup>
          <m:mo>+</m:mo><m:mfrac>
           <m:mrow>
            <m:msqrt>
             <m:mn>3</m:mn>
            </m:msqrt>

           </m:mrow>
           <m:mn>2</m:mn>
          </m:mfrac>
          <m:msup>
           <m:mi>y</m:mi>
           <m:mo>′</m:mo>
          </m:msup>
          <m:mo>−</m:mo><m:mn>1</m:mn>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>

     </m:mrow>
    </m:math>
    </para>

    <media id="fs-id2322951" alt="" display="block">
     <image mime-type="image/jpg" src="CNX_Precalc_Figure_10_04_207.jpg"/>
    </media>

</solution></exercise>
