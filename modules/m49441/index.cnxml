      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>10</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>16</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>28</m:mn><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo>
