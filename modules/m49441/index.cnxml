</solution></exercise>
   <exercise id="fs-id2245306"><problem id="fs-id2245307"><para id="fs-id2245308"> <m:math>
    <m:mrow>
     <m:mi>y</m:mi><m:mo>=</m:mo><m:mfrac>
      <m:mrow>
       <m:msup>
        <m:mi>x</m:mi>
