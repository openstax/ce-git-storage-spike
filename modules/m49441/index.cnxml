      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>12</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id1277151">
     <para id="fs-id1277153"> <m:math>
      <m:mrow>
       <m:mi>A</m:mi><m:mi>B</m:mi><m:mo>=</m:mo><m:mn>6</m:mn><m:mo>&gt;</m:mo><m:mn>0</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:math> ellipse</para></solution></exercise>
   <exercise id="fs-id1277179"><problem id="fs-id1277180"><para id="fs-id1277181"> <m:math>
    <m:mrow>
     <m:mn>4</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>9</m:mn><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>36</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>125</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id1418848"><problem id="fs-id1418849"><para id="fs-id1418850"> <m:math>
    <m:mrow>
     <m:mn>3</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>6</m:mn><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>36</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>125</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id2433640">
     <para id="fs-id2433642"> <m:math>
      <m:mrow>
       <m:msup>
        <m:mi>B</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>A</m:mi><m:mi>C</m:mi><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:math> parabola</para></solution></exercise>
   <exercise id="fs-id2352771"><problem id="fs-id2352772"><para id="fs-id2352774"> <m:math>
    <m:mrow>
     <m:mo>−</m:mo><m:mn>3</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>3</m:mn><m:msqrt>
      <m:mn>3</m:mn>
     </m:msqrt>
     <m:mi>x</m:mi><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>4</m:mn><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>9</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id2472798"><problem id="fs-id2472799"><para id="fs-id2472800"> <m:math>
    <m:mrow>
     <m:mn>2</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>4</m:mn><m:msqrt>
      <m:mn>3</m:mn>
     </m:msqrt>
     <m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>6</m:mn><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>6</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id2861567">
     <para id="fs-id2861569"> <m:math>
      <m:mrow>
       <m:msup>
        <m:mi>B</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>A</m:mi><m:mi>C</m:mi><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:math> parabola</para></solution></exercise>
   <exercise id="fs-id2155989"><problem id="fs-id2155990"><para id="fs-id2155991"> <m:math>
    <m:mrow>
     <m:mo>−</m:mo><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>4</m:mn><m:msqrt>
      <m:mn>2</m:mn>
     </m:msqrt>
     <m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>2</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id2161610"><problem id="fs-id2161611"><para id="fs-id2161612"> <m:math>
    <m:mrow>
     <m:mn>8</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>4</m:mn><m:msqrt>
      <m:mn>2</m:mn>
     </m:msqrt>
     <m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>10</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id1940128">
     <para id="fs-id1940130"> <m:math>
      <m:mrow>
       <m:msup>
        <m:mi>B</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:mn>4</m:mn><m:mi>A</m:mi><m:mi>C</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>96</m:mn><m:mo>&lt;</m:mo><m:mn>0</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:math> ellipse</para></solution></exercise>

   <para id="fs-id1940172">For the following exercises, find a new representation of the given equation after rotating through the given angle.</para>
   <exercise id="fs-id1940176"><problem id="fs-id1940177"><para id="fs-id1940178"> <m:math>
 <m:mrow>
  <m:mn>3</m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:msup>
   <m:mi>y</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mn>5</m:mn><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>45°</m:mn>
 </m:mrow>
</m:math>
</para></problem></exercise>
   <exercise id="fs-id2056389"><problem id="fs-id2056390"><para id="fs-id2056391"> <m:math>
 <m:mrow>
  <m:mn>4</m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:msup>
   <m:mi>y</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mn>2</m:mn><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>45°</m:mn>
 </m:mrow>
</m:math>
</para></problem>
   <solution id="fs-id1888346">
    <para id="fs-id1888348"> <m:math>
     <m:mrow>
      <m:mn>7</m:mn><m:msup>
       <m:msup>
        <m:mi>x</m:mi>
        <m:mo>′</m:mo>
       </m:msup>

       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>9</m:mn><m:msup>
       <m:msup>
        <m:mi>y</m:mi>
        <m:mo>′</m:mo>
       </m:msup>

       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para>
</solution></exercise>
   <exercise id="fs-id1753645"><problem id="fs-id1753646"><para id="fs-id1753647"> <m:math>
 <m:mrow>
  <m:mn>2</m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>30°</m:mn>
 </m:mrow>
</m:math>
</para></problem></exercise>
   <exercise id="fs-id1558064"><problem id="fs-id1558065"><para id="fs-id1558066"> <m:math>
 <m:mrow>
  <m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>45°</m:mn>
 </m:mrow>
</m:math>
</para></problem>
    <solution id="fs-id1799395">
     <para id="fs-id1799397"> <m:math>
      <m:mrow>
       <m:mn>3</m:mn><m:msup>
        <m:msup>
         <m:mi>x</m:mi>
         <m:mo>′</m:mo>
        </m:msup>

        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>+</m:mo><m:mn>2</m:mn><m:msup>
        <m:mi>x</m:mi>
        <m:mo>′</m:mo>
       </m:msup>
       <m:msup>
        <m:mi>y</m:mi>
        <m:mo>′</m:mo>
       </m:msup>
       <m:mo>−</m:mo><m:mn>5</m:mn><m:msup>
        <m:msup>
         <m:mi>y</m:mi>
         <m:mo>′</m:mo>
        </m:msup>

        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
      </m:mrow>
     </m:math>
     </para></solution></exercise>
   <exercise id="fs-id1587288"><problem id="fs-id1587289"><para id="fs-id1587290"> <m:math>
 <m:mrow>
  <m:mn>4</m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:msqrt>
   <m:mn>2</m:mn>
  </m:msqrt>
  <m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:msup>
   <m:mi>y</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>45°</m:mn>
 </m:mrow>
</m:math>
</para></problem></exercise>

   <para id="fs-id2881099">For the following exercises, determine the angle <m:math>
    <m:mrow>
     <m:mi>θ</m:mi>
    </m:mrow>
   </m:math> that will eliminate the <m:math>
    <m:mrow>
     <m:mi>x</m:mi><m:mi>y</m:mi>
    </m:mrow>
   </m:math> term and write the corresponding equation without the <m:math>
    <m:mrow>
     <m:mi>x</m:mi><m:mi>y</m:mi>
    </m:mrow>
   </m:math> term.</para>
   <exercise id="fs-id1806812"><problem id="fs-id1806813"><para id="fs-id1806814"> <m:math>
    <m:mrow>
     <m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>3</m:mn><m:msqrt>
      <m:mn>3</m:mn>
     </m:msqrt>
     <m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>2</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem>
   <solution id="fs-id2451646">
    <para id="fs-id2451648"> <m:math>
     <m:mrow>
      <m:mi>θ</m:mi><m:mo>=</m:mo><m:msup>
       <m:mrow>
        <m:mn>60</m:mn>
       </m:mrow>
       <m:mo>∘</m:mo>
      </m:msup>
      <m:mo>,</m:mo><m:mn>11</m:mn><m:msup>
       <m:msup>
        <m:mi>x</m:mi>
        <m:mo>′</m:mo>
       </m:msup>

       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>−</m:mo><m:msup>
       <m:msup>
        <m:mi>y</m:mi>
        <m:mo>′</m:mo>
       </m:msup>

       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:msqrt>
       <m:mn>3</m:mn>
      </m:msqrt>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mo>′</m:mo>
      </m:msup>
      <m:mo>+</m:mo><m:msup>
       <m:mi>y</m:mi>
       <m:mo>′</m:mo>
      </m:msup>
      <m:mo>−</m:mo><m:mn>4</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:math>
    </para>
</solution></exercise>
