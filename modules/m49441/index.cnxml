   <exercise id="fs-id2787375"><problem id="fs-id2787376"><para id="fs-id2787377"> <m:math>
    <m:mrow>
     <m:mfrac>
      <m:mrow>
       <m:msup>
        <m:mi>x</m:mi>
        <m:mn>2</m:mn>
       </m:msup>

      </m:mrow>
      <m:mn>4</m:mn>
     </m:mfrac>
     <m:mo>+</m:mo><m:mfrac>
      <m:mrow>
       <m:msup>
        <m:mi>y</m:mi>
        <m:mn>2</m:mn>
       </m:msup>

      </m:mrow>
      <m:mn>1</m:mn>
     </m:mfrac>
     <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mi>θ</m:mi><m:mo>=</m:mo><m:msup>
