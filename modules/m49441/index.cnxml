        </m:msup>

        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>−</m:mo><m:msqrt>
        <m:mn>2</m:mn>
       </m:msqrt>
       <m:msup>
        <m:mi>x</m:mi>
        <m:mo>′</m:mo>
       </m:msup>
       <m:mo>+</m:mo><m:msqrt>
        <m:mn>2</m:mn>
       </m:msqrt>
       <m:msup>
        <m:mi>y</m:mi>
        <m:mo>′</m:mo>
       </m:msup>
       <m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
