      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>6</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>2</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id2584248"><problem id="fs-id2584249"><para id="fs-id2584250"> <m:math>
    <m:mrow>
     <m:mn>4</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id2474745">
     <para id="fs-id2474747"> <m:math>
      <m:mrow>
       <m:mi>A</m:mi><m:mi>B</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>4</m:mn><m:mo>&lt;</m:mo><m:mn>0</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:math> hyperbola</para></solution></exercise>
   <exercise id="fs-id2474775"><problem id="fs-id2474776"><para id="fs-id2474777"> <m:math>
    <m:mrow>
     <m:mn>4</m:mn><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mo>−</m:mo><m:mn>5</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>9</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id2927799"><problem id="fs-id2927800"><para id="fs-id2927801"> <m:math>
    <m:mrow>
     <m:mn>2</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2</m:mn>
     </m:msup>
