     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id2252418"><para id="fs-id2252420">perpendicular</para></solution>
</exercise>
<exercise id="fs-id2252424"><problem id="fs-id2252425"><para id="fs-id2252426"> <m:math>
 <m:mrow>
  <m:mtable>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:mn>3</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>12</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:mo>−</m:mo><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math> </para></problem></exercise>
 <exercise id="fs-id2261858"><problem id="fs-id2261859"><para id="fs-id2261860"> <m:math>
 <m:mrow>
  <m:mtable>
