   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mi>y</m:mi><m:mo>−</m:mo><m:mi>int</m:mi><m:mo>:</m:mo><m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mn>2</m:mn><m:mo stretchy="false">)</m:mo>
   </m:mtd>
  </m:mtr>
  <m:mtr>
