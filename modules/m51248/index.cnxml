<exercise id="fs-id1167339298131"><problem id="fs-id1167339298132"><para id="fs-id1167339298133"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mi>a</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:mi>b</m:mi>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mn>3</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mi>b</m:mi>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1167339298181"><para id="fs-id1167339298182"> <m:math>
 <m:mrow>
  <m:mn>3</m:mn><m:msup>
   <m:mi>a</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>5</m:mn><m:mi>a</m:mi><m:mi>b</m:mi><m:mo>−</m:mo><m:mn>2</m:mn><m:msup>
   <m:mi>b</m:mi>
   <m:mn>2</m:mn>
  </m:msup>

 </m:mrow>
</m:math>
</para></solution></exercise>
