    <row>
      <entry namest="c1" nameend="c4">Scoring</entry>
    </row>
  <row>
    <entry namest="c1" nameend="c4" align="left">Total Number in Each Column</entry>
  </row>
</thead>
<tbody>
  <row>
    <entry align="left"><emphasis>Scoring:</emphasis></entry>
    <entry align="left"><emphasis>Always: 4 points each</emphasis></entry>
    <entry align="left"><emphasis>Sometimes: 2 points each</emphasis></entry>
    <entry align="left"><emphasis>Never: 0 points each</emphasis></entry>
 </row>
  <row>
    <entry align="left"><emphasis>Total points:</emphasis></entry>
    <entry align="left"/>
