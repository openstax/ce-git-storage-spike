   <exercise id="fs-id1561566"><problem id="fs-id1561567"><para id="fs-id3633665"> <m:math>
    <m:mrow>
     <m:mn>9</m:mn><m:mspace width="0.3em"/><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>ω</m:mi><m:mo>−</m:mo><m:mn>2</m:mn><m:mo>=</m:mo><m:mn>4</m:mn><m:mspace width="0.8em"/><m:msup>
