        <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mi>t</m:mi>
         <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>a</m:mi><m:msup>
          <m:mi>e</m:mi>
          <m:mrow>
           <m:mo>−</m:mo><m:mi>c</m:mi>
          </m:mrow>
         </m:msup>
        <m:msup>
         <m:mrow/>
         <m:mi>t</m:mi>
        </m:msup>
        <m:mi>sin</m:mi><m:mo stretchy="false">(</m:mo><m:mi>ω</m:mi><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mspace width="0.8em"/><m:mtext>or</m:mtext><m:mspace width="0.8em"/><m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mi>t</m:mi>
