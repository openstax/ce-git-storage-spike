       <m:mrow>
        <m:mn>2</m:mn><m:mi>θ</m:mi>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
    </m:mrow>
   </m:math> and <m:math>
    <m:mrow>
     <m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mn>2</m:mn><m:mi>θ</m:mi>
      </m:mrow>
      <m:mo>)</m:mo></m:mrow>    </m:mrow>
   </m:math> given <m:math>
    <m:mrow>
     <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
      <m:mn>1</m:mn>
      <m:mn>3</m:mn>
     </m:mfrac>
         </m:mrow>
   </m:math> and <m:math>
    <m:mrow>
     <m:mi>θ</m:mi>    </m:mrow>
   </m:math> is in the interval <m:math>
    <m:mrow>
     <m:mrow><m:mo>[</m:mo> <m:mrow>
      <m:mfrac>
       <m:mi>π</m:mi>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>,</m:mo><m:mi>π</m:mi>
     </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>.</m:mo>
    </m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id1681784"><problem id="fs-id1681785"><para id="fs-id1681786">Find <m:math>
    <m:mrow>
     <m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mn>2</m:mn><m:mi>θ</m:mi>
      </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mo>, </m:mo><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>2</m:mn><m:mi>θ</m:mi>
       </m:mrow>
       <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
    </m:mrow>
   </m:math> and <m:math>
    <m:mrow>
     <m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mn>2</m:mn><m:mi>θ</m:mi>
      </m:mrow>
      <m:mo>)</m:mo></m:mrow>    </m:mrow>
   </m:math> given <m:math>
    <m:mrow>
     <m:mi>sec</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
      <m:mn>5</m:mn>
      <m:mn>3</m:mn>
     </m:mfrac>
         </m:mrow>
   </m:math> and <m:math>
    <m:mrow>
     <m:mi>θ</m:mi>    </m:mrow>
   </m:math> is in the interval <m:math>
    <m:mrow>
     <m:mrow><m:mo>[</m:mo> <m:mrow>
      <m:mfrac>
       <m:mi>π</m:mi>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>,</m:mo><m:mi>π</m:mi>
     </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>.</m:mo>
    </m:mrow>
   </m:math>
   </para></problem>
   <solution id="fs-id1362528">
