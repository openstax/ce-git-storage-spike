     <m:mrow>
      <m:mo>−</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>24</m:mn>
       </m:mrow>
       <m:mrow>
        <m:mn>25</m:mn>
       </m:mrow>
      </m:mfrac>
      <m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
       <m:mn>7</m:mn>
       <m:mrow>
        <m:mn>25</m:mn>
       </m:mrow>
      </m:mfrac>
      <m:mo>,</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>24</m:mn>
       </m:mrow>
       <m:mn>7</m:mn>
      </m:mfrac>

     </m:mrow>
    </m:math>
    </para>
</solution></exercise>
