     </m:mrow>
    </m:mtd>

   </m:mtr>
   <m:mtr columnalign="right">
    <m:mtd columnalign="right"> </m:mtd>

   <m:mtd columnalign="left">
<m:mrow>
      <m:mo>=</m:mo><m:mtext>tan</m:mtext><m:mi>x</m:mi><m:mo>(</m:mo><m:msup><m:mtext>tan</m:mtext><m:mn>2</m:mn></m:msup><m:mi>x</m:mi><m:mo>–</m:mo><m:mn>1</m:mn><m:mo>–</m:mo><m:msup><m:mtext>tan</m:mtext><m:mn>2</m:mn></m:msup><m:mi>x</m:mi><m:mo>)</m:mo>
     </m:mrow>
    </m:mtd>



   </m:mtr>
