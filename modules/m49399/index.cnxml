    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mn>0.85</m:mn>
      </m:mrow>
      <m:mo>)</m:mo></m:mrow>
    </m:mrow>
    <m:mi>t</m:mi>
   </m:msup>
