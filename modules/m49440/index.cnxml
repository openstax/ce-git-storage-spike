<m:msup>
<m:mrow>
<m:mrow>
<m:mo>(</m:mo>
<m:mrow>
<m:mi>x</m:mi>
<m:mo>−</m:mo>
<m:mi>h</m:mi>
</m:mrow>
<m:mo>)</m:mo>
</m:mrow>
</m:mrow>
<m:mn>2</m:mn>
</m:msup>
<m:mo>+</m:mo>
