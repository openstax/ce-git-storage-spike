                    <m:mrow>
                      <m:mi>h</m:mi><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mi>k</m:mi><m:mo>+</m:mo><m:mi>p</m:mi>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
              </m:math>
              </entry>
              <entry align="center"> <m:math>
                <m:mrow>
                  <m:mi>y</m:mi><m:mo>=</m:mo><m:mi>k</m:mi><m:mo>−</m:mo><m:mi>p</m:mi>
                </m:mrow>
              </m:math>
              </entry>
              <entry align="center"> <m:math>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>h</m:mi><m:mo>±</m:mo><m:mn>2</m:mn><m:mi>p</m:mi><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mi>k</m:mi><m:mo>+</m:mo><m:mi>p</m:mi>
