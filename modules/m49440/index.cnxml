                <m:mrow>
                  <m:msup>
                    <m:mi>x</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mo>=</m:mo><m:mn>4</m:mn><m:mi>p</m:mi><m:mi>y</m:mi>
                </m:mrow>
              </m:math>
              </entry>
              <entry align="center"> <m:math>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mn>0</m:mn><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mi>p</m:mi>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
              </m:math>
              </entry>
