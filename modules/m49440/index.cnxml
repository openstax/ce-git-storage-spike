                </m:mrow>
              </m:math>
              </entry>
              <entry align="center"> <m:math>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>p</m:mi><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mn>0</m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
              </m:math>
              </entry>
              <entry align="center"> <m:math>
                <m:mrow>
                  <m:mi>x</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mi>p</m:mi>
                </m:mrow>
              </m:math>
              </entry>
              <entry align="center"> <m:math>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>p</m:mi><m:mo>,</m:mo><m:mspace width="0.8em"/><m:mtext/><m:mo>±</m:mo><m:mn>2</m:mn><m:mi>p</m:mi>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
              </m:math>
              </entry>
            </row>
