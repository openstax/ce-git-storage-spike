<exercise id="fs-id1165135417965">
<problem id="fs-id1165135417967">
<para id="fs-id1165135417969">Find an equation that could be represented by <link target-id="CNX_Precalc_Figure_12_02_202"/>.</para>
<figure id="CNX_Precalc_Figure_12_02_202" class="small">
<media id="fs-id1165135417955" alt="Graph of increasing function with a removable discontinuity at (-3, -1).">
<image mime-type="image/jpg" src="CNX_Precalc_Figure_12_02_202.jpg"/>
</media>
</figure>
</problem>
<solution id="fs-id1165135417976">
<para id="fs-id1165135417978"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>5</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para>
</solution>
</exercise>

