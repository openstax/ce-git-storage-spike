 </m:mrow>
</m:math>
</para></problem>
</exercise>

<exercise id="fs-id1165135331069">
<problem id="fs-id1165135331071">
<para id="fs-id1165135331073"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
   <m:mn>1</m:mn>
   <m:mi>x</m:mi>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165135331106">
<para id="fs-id1165135331108"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>1</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo stretchy="false">)</m:mo>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></solution>
</exercise>

<exercise id="fs-id1165135417736">
<problem id="fs-id1165135417738">
<para id="fs-id1165135417741"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
   <m:mn>1</m:mn>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></problem>
</exercise>

<exercise id="fs-id1165135417784">
<problem id="fs-id1165135417786">
<para id="fs-id1165135417789"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msqrt>
   <m:mi>x</m:mi>
  </m:msqrt>

 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165135417869">
<para id="fs-id1165135417871"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>1</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msqrt>
     <m:mrow>
      <m:mi>x</m:mi><m:mo>+</m:mo><m:mi>h</m:mi>
     </m:mrow>
    </m:msqrt>
    <m:mo>+</m:mo><m:msqrt>
     <m:mi>x</m:mi>
    </m:msqrt>

   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></solution>
</exercise>
</section>

