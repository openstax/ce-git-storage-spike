  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>2</m:mn><m:msup>
        <m:mi>x</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>≤</m:mo><m:mn>0</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>&gt;</m:mo><m:mn>0</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> </m:mrow><m:mtext>; </m:mtext><m:munder>
   <m:mrow>
    <m:mi>lim</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:msup>
     <m:mn>0</m:mn>
     <m:mo>+</m:mo>
    </m:msup>

   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165135415682">
<para id="fs-id1165135415684"> <m:math>
 <m:mrow>
  <m:mo>−</m:mo><m:mn>3</m:mn>
 </m:mrow>
</m:math>
</para>
</solution>
</exercise>

<exercise id="fs-id1165135415700">
<problem id="fs-id1165135415702">
<para id="fs-id1165135415704"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>2</m:mn><m:msup>
        <m:mi>x</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>≤</m:mo><m:mn>0</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>&gt;</m:mo><m:mn>0</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> </m:mrow><m:mtext>; </m:mtext><m:munder>
   <m:mrow>
    <m:mi>lim</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:msup>
     <m:mn>0</m:mn>
     <m:mo>−</m:mo>
    </m:msup>

   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math> </para></problem>
</exercise>

<exercise id="fs-id1165135415855">
<problem id="fs-id1165135415858">
<para id="fs-id1165135415860"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>2</m:mn><m:msup>
        <m:mi>x</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>≤</m:mo><m:mn>0</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>&gt;</m:mo><m:mn>0</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> </m:mrow><m:mtext>; </m:mtext><m:munder>
   <m:mrow>
    <m:mi>lim</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mn>0</m:mn>
   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165135403520">
<para id="fs-id1165135403522">does not exist; right-hand limit is not the same as the left-hand limit.</para>
</solution>
</exercise>

<exercise id="fs-id1165135403527">
<problem id="fs-id1165135403529">
<para id="fs-id1165135403532"> <m:math>
 <m:mrow>
  <m:munder>
