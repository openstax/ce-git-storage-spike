   <m:mn>6</m:mn>
  </m:mfrac>
  </m:mrow>
</m:math>
</para></problem><solution id="fs-id2262276"><para id="fs-id2262278"> <m:math>
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:msqrt>
      <m:mn>3</m:mn>
     </m:msqrt>

    </m:mrow>
    <m:mn>3</m:mn>
   </m:mfrac>

  </m:mrow>
 </m:math>
 </para></solution></exercise>
