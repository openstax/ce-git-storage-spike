<example id="Example_07_04_09">
<exercise id="fs-id2136898">
<problem id="fs-id2136900">
<title>Finding the Value of Trigonometric Functions</title>
<para id="fs-id2363901">If <m:math>
 <m:mrow>
  <m:mi>sin</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
   <m:mrow>
    <m:msqrt>
     <m:mn>3</m:mn>
    </m:msqrt>

   </m:mrow>
   <m:mn>2</m:mn>
  </m:mfrac>
  <m:mspace width="0.3em"/><m:mtext>and</m:mtext><m:mspace width="0.3em"/><m:mtext>cos</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
   <m:mn>1</m:mn>
   <m:mn>2</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mtext>find</m:mtext><m:mspace width="0.3em"/><m:mtext>sec</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mtext>csc</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mtext>tan</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mtext>cot</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id2674302">
<equation id="fs-id2674304" class="unnumbered"><label/>
 <m:math display="block">
 <m:mrow>
  <m:mtable>
   <m:mtr>
    <m:mtd columnalign="right">
     <m:mrow>
      <m:mtext>sec </m:mtext><m:mi>t</m:mi></m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mtext>cos </m:mtext><m:mi>t</m:mi></m:mrow>
      </m:mfrac>
      </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mfrac>
         <m:mn>1</m:mn>
         <m:mn>2</m:mn>
        </m:mfrac>
        </m:mrow>
      </m:mfrac>
      <m:mo>=</m:mo><m:mn>2</m:mn></m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd columnalign="right">
     <m:mrow>
      <m:mtext>csc </m:mtext><m:mi>t</m:mi></m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mtext>sin </m:mtext><m:mi>t</m:mi></m:mrow>
      </m:mfrac>
      </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mo>−</m:mo><m:mfrac>
         <m:mrow>
          <m:msqrt>
           <m:mn>3</m:mn>
          </m:msqrt>
          </m:mrow>
         <m:mn>2</m:mn>
        </m:mfrac>
        </m:mrow>
      </m:mfrac>
      <m:mo>−</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>2</m:mn><m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>
        </m:mrow>
       <m:mn>3</m:mn>
      </m:mfrac>
      </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd columnalign="right">
     <m:mrow>
      <m:mtext>tan </m:mtext><m:mi>t</m:mi></m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:mtext>sin </m:mtext><m:mi>t</m:mi></m:mrow>
       <m:mrow>
        <m:mtext>cos </m:mtext><m:mi>t</m:mi></m:mrow>
      </m:mfrac>
      </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:mo>−</m:mo><m:mfrac>
         <m:mrow>
          <m:msqrt>
           <m:mn>3</m:mn>
          </m:msqrt>
          </m:mrow>
         <m:mn>2</m:mn>
        </m:mfrac>
        </m:mrow>
       <m:mrow>
        <m:mfrac>
         <m:mn>1</m:mn>
         <m:mn>2</m:mn>
        </m:mfrac>
        </m:mrow>
      </m:mfrac>
      <m:mo>=</m:mo><m:mo>−</m:mo><m:msqrt>
       <m:mn>3</m:mn>
      </m:msqrt>
      </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd columnalign="right">
     <m:mrow>
      <m:mtext>cot </m:mtext><m:mi>t</m:mi></m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mtext>tan </m:mtext><m:mi>t</m:mi></m:mrow>
      </m:mfrac>
      </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mo>−</m:mo><m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>
        </m:mrow>
      </m:mfrac>
      <m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac>
       <m:mrow>
        <m:msqrt>
         <m:mn>3</m:mn>
        </m:msqrt>
        </m:mrow>
       <m:mn>3</m:mn>
      </m:mfrac>
      </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable></m:mrow>
</m:math>

</equation></solution>
</exercise>
</example>

<note id="fs-id2028568" class="precalculus try">
<label>Try It</label>
<exercise id="ti_07_04_09">
<problem id="fs-id1735745"><para id="fs-id1735746"> <m:math>
 <m:mrow>
  <m:mi>sin</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:msqrt>
     <m:mn>2</m:mn>
    </m:msqrt>

   </m:mrow>
   <m:mn>2</m:mn>
  </m:mfrac>
  <m:mspace width="0.3em"/><m:mtext>and</m:mtext><m:mspace width="0.3em"/><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:msqrt>
     <m:mn>2</m:mn>
    </m:msqrt>

   </m:mrow>
   <m:mn>2</m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mtext>find</m:mtext><m:mspace width="0.3em"/><m:mtext>sec</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mtext>csc</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mtext>tan</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mtext>and</m:mtext><m:mspace width="0.3em"/><m:mtext>cot</m:mtext><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id2756064"><para id="fs-id2756065"> <m:math>
 <m:mrow>
  <m:mi>sec</m:mi><m:mi>t</m:mi><m:mo>=</m:mo><m:msqrt>
   <m:mn>2</m:mn>
  </m:msqrt>
  <m:mo>,</m:mo><m:mi>csc</m:mi><m:mi>t</m:mi><m:mo>=</m:mo><m:msqrt>
   <m:mn>2</m:mn>
  </m:msqrt>
  <m:mo>,</m:mo><m:mi>tan</m:mi><m:mi>t</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mi>cot</m:mi><m:mi>t</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
 </m:mrow>
</m:math> </para></solution>
</exercise>
</note>
