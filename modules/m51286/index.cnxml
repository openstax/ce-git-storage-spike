   <m:mn>4</m:mn>
  </m:mfrac>
  </m:mrow>
</m:math>
</para></problem>
  <solution id="fs-id2627123"><para id="fs-id2627124"> <m:math>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:msqrt>
       <m:mn>2</m:mn>
      </m:msqrt>

     </m:mrow>
     <m:mn>2</m:mn>
    </m:mfrac>

   </m:mrow>
  </m:math>
  </para></solution></exercise>
 <exercise id="fs-id2216404"><problem id="fs-id2216405"><para id="fs-id2216406"> <m:math>
 <m:mrow>
  <m:mi>cot</m:mi><m:mspace width="0.3em"/><m:mfrac>
