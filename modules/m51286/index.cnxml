<exercise id="ti_07_04_02">
<problem id="fs-id1570497"><para id="fs-id1497298">Find <m:math>
 <m:mrow>
  <m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>sec</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>csc</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>,</m:mo></m:mrow>
</m:math> and <m:math>
 <m:mrow>
  <m:mi>cot</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>.</m:mo></m:mrow>
</m:math> when <m:math>
<m:mrow>
<m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
<m:mi>π</m:mi>
<m:mn>3</m:mn>
</m:mfrac>
<m:mo>.</m:mo>
</m:mrow>
</m:math>
</para></problem>
<solution id="fs-id2134071">
<para id="fs-id1556049">
 <m:math>
<m:mtable>
<m:mtr>
<m:mtd columnalign="left">
<m:mi>sin</m:mi><m:mfrac>
<m:mi>π</m:mi>
<m:mn>3</m:mn>
</m:mfrac>
<m:mo>=</m:mo><m:mfrac>
<m:mrow>
<m:msqrt>
<m:mn>3</m:mn>
</m:msqrt>

</m:mrow>
<m:mn>2</m:mn>
</m:mfrac>

</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd columnalign="left">
<m:mi>cos</m:mi><m:mfrac>
<m:mi>π</m:mi>
<m:mn>3</m:mn>
</m:mfrac>
<m:mo>=</m:mo><m:mfrac>
<m:mn>1</m:mn>
<m:mn>2</m:mn>
</m:mfrac>

</m:mtd>
