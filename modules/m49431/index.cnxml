    <solution id="fs-id1165133058925">
     <para id="fs-id1165133058927"> <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mfrac>
          <m:mrow>
           <m:msqrt>
