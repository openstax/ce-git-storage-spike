        </m:msqrt>

       </m:mrow>
       <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>−2</m:mn><m:msqrt>
          <m:mrow>
           <m:mfrac>
            <m:mrow>
             <m:mn>70</m:mn>
            </m:mrow>
            <m:mrow>
             <m:mn>383</m:mn>
            </m:mrow>
           </m:mfrac>

          </m:mrow>
         </m:msqrt>
         <m:mo>,</m:mo><m:mn>2</m:mn><m:msqrt>
          <m:mrow>
           <m:mfrac>
            <m:mrow>
             <m:mn>35</m:mn>
            </m:mrow>
            <m:mrow>
             <m:mn>29</m:mn>
            </m:mrow>
           </m:mfrac>

          </m:mrow>
         </m:msqrt>

        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>2</m:mn><m:msqrt>
           <m:mrow>
            <m:mfrac>
             <m:mrow>
              <m:mn>70</m:mn>
             </m:mrow>
             <m:mrow>
              <m:mn>383</m:mn>
             </m:mrow>
            </m:mfrac>

           </m:mrow>
          </m:msqrt>
          <m:mo>,</m:mo><m:mn>−2</m:mn><m:msqrt>
           <m:mrow>
            <m:mfrac>
             <m:mrow>
              <m:mn>35</m:mn>
             </m:mrow>
             <m:mrow>
              <m:mn>29</m:mn>
             </m:mrow>
            </m:mfrac>

           </m:mrow>
          </m:msqrt>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mn>2</m:mn><m:msqrt>
            <m:mrow>
             <m:mfrac>
              <m:mrow>
               <m:mn>70</m:mn>
              </m:mrow>
              <m:mrow>
               <m:mn>383</m:mn>
              </m:mrow>
             </m:mfrac>

            </m:mrow>
           </m:msqrt>
           <m:mo>,</m:mo><m:mn>2</m:mn><m:msqrt>
            <m:mrow>
