   <exercise id="fs-id1165134058210"><problem id="fs-id1165134058211"><para id="fs-id1165134058212"> <m:math>
 <m:mrow>
  <m:mtable columnalign="left">
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mn>−6</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>7</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>         </m:mtext><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
    <solution id="fs-id1165135367599">
     <para id="fs-id1165135367602"> <m:math>
      <m:mrow>
       <m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:msqrt>
          <m:mrow>
           <m:mfrac>
            <m:mn>1</m:mn>
            <m:mn>2</m:mn>
           </m:mfrac>
           <m:mrow><m:mo>(</m:mo>
            <m:mrow>
             <m:msqrt>
              <m:mrow>
               <m:mn>73</m:mn>
              </m:mrow>
             </m:msqrt>
             <m:mn>−5</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
         </m:msqrt>
         <m:mo>,</m:mo><m:mfrac>
          <m:mn>1</m:mn>
          <m:mn>2</m:mn>
         </m:mfrac>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mn>7</m:mn><m:mo>−</m:mo><m:msqrt>
            <m:mrow>
             <m:mn>73</m:mn>
            </m:mrow>
           </m:msqrt>

          </m:mrow>
          <m:mo>)</m:mo></m:mrow>
        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:msqrt>
           <m:mrow>
            <m:mfrac>
             <m:mn>1</m:mn>
             <m:mn>2</m:mn>
            </m:mfrac>
            <m:mrow><m:mo>(</m:mo>
             <m:mrow>
              <m:msqrt>
               <m:mrow>
                <m:mn>73</m:mn>
               </m:mrow>
              </m:msqrt>
              <m:mn>−5</m:mn>
             </m:mrow>
             <m:mo>)</m:mo></m:mrow>
           </m:mrow>
          </m:msqrt>
          <m:mo>,</m:mo><m:mfrac>
           <m:mn>1</m:mn>
           <m:mn>2</m:mn>
          </m:mfrac>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mn>7</m:mn><m:mo>−</m:mo><m:msqrt>
             <m:mrow>
              <m:mn>73</m:mn>
             </m:mrow>
            </m:msqrt>

           </m:mrow>
           <m:mo>)</m:mo></m:mrow>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math>
     </para>
    </solution></exercise>
