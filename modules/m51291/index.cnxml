                </m:mrow>
                <m:mo>)</m:mo></m:mrow>
            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo><m:mn>−3</m:mn><m:mspace width="0.3em"/><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>10</m:mn><m:mi>x</m:mi>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mi>csc</m:mi><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mn>6</m:mn><m:mi>x</m:mi>
              </m:mrow>
