 <m:mtd rowalign="center">
  <m:mo>=</m:mo>
 </m:mtd>
 <m:mtd columnalign="left" rowalign="center">
  <m:mrow>
   <m:mfrac>
    <m:mrow>
     <m:mo>−</m:mo><m:mfrac>
      <m:mrow>
       <m:mn>16</m:mn>
      </m:mrow>
