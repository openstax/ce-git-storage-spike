   <m:mn>3</m:mn>
  </m:msup>
  <m:mn>−32</m:mn><m:msup>
   <m:mi>t</m:mi>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>−</m:mo><m:mi>t</m:mi><m:mo>+</m:mo><m:mn>7</m:mn>
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1167339138836"><problem id="fs-id1167339138837"><para id="fs-id1167339138838"> <m:math>
