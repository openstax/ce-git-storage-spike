 <m:mrow>
  <m:mo stretchy="false">(</m:mo><m:mn>9</m:mn><m:mi>m</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mi>m</m:mi><m:mo>+</m:mo><m:mn>8</m:mn><m:mo stretchy="false">)</m:mo>
 </m:mrow>
