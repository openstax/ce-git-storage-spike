<exercise id="fs-id1167339214002"><problem id="fs-id1167339214003"><para id="fs-id1167339214004"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mrow>
    <m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>t</m:mi><m:mo>−</m:mo><m:mn>5</m:mn><m:mi>u</m:mi><m:mo stretchy="false">)</m:mo>
