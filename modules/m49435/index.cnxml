         <m:mtd columnalign="right">
          <m:mn>1</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
        </m:mtr>
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>1</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
        </m:mtr>
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>1</m:mn>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</equation></commentary>
        </exercise></example>

      <note id="fs-id1165137446254" class="precalculus try">
        <label>Try It</label>
        <exercise id="ti_09_07_03">
          <problem id="fs-id1165135536370"><para id="fs-id1165135536371">Find the inverse of the <m:math>
            <m:mrow>
              <m:mn>3</m:mn><m:mo>×</m:mo><m:mn>3</m:mn>
            </m:mrow>
          </m:math> matrix.</para>
            <equation id="fs-id1165137732890" class="unnumbered"><label/> <m:math display="block">
              <m:mrow>
                <m:mi>A</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
                  <m:mtable>
                    <m:mtr>
                      <m:mtd>
                        <m:mrow>
                          <m:mspace width="0.8em"/><m:mn>2</m:mn>
                        </m:mrow>
                      </m:mtd>
                      <m:mtd>
                        <m:mrow>
                          <m:mn>−17</m:mn>
                        </m:mrow>
                      </m:mtd>
                      <m:mtd>
                        <m:mrow>
                          <m:mn>11</m:mn>
                        </m:mrow>
                      </m:mtd>
                    </m:mtr>
                    <m:mtr>
                      <m:mtd>
                        <m:mrow>
                          <m:mn>−1</m:mn>
                        </m:mrow>
                      </m:mtd>
                      <m:mtd>
                        <m:mrow>
                          <m:mspace width="0.8em"/><m:mn>11</m:mn>
                        </m:mrow>
                      </m:mtd>
                      <m:mtd>
                        <m:mrow>
                          <m:mn>−7</m:mn>
                        </m:mrow>
                      </m:mtd>
                    </m:mtr>
                    <m:mtr>
                      <m:mtd>
                        <m:mrow>
                          <m:mspace width="0.8em"/><m:mn>0</m:mn>
                        </m:mrow>
                      </m:mtd>
                      <m:mtd>
                        <m:mrow>
                          <m:mspace width="0.8em"/><m:mn>3</m:mn>
                        </m:mrow>
                      </m:mtd>
                      <m:mtd>
                        <m:mrow>
                          <m:mn>−2</m:mn>
                        </m:mrow>
                      </m:mtd>
                    </m:mtr>

                  </m:mtable>
                </m:mrow> <m:mo>]</m:mo></m:mrow>
              </m:mrow>
            </m:math>
            </equation>
          </problem>
          <solution id="fs-id1165135478197"><para id="fs-id1165135478198"> <m:math>
            <m:mrow>
              <m:msup>
                <m:mi>A</m:mi>
                <m:mrow>
                  <m:mn>−1</m:mn>
