        <para id="fs-id1165135388626">So,</para>
          <equation id="fs-id1165135388629" class="unnumbered"><label/> <m:math display="block">
            <m:mrow>
              <m:msup>
                <m:mi>A</m:mi>
                <m:mrow>
                  <m:mn>−1</m:mn>
                </m:mrow>
              </m:msup>
              <m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
                <m:mtable>
                  <m:mtr>
                    <m:mtd>
                      <m:mrow>
                        <m:mn>−2</m:mn>
                      </m:mrow>
                    </m:mtd>
                    <m:mtd>
                      <m:mrow>
                        <m:mn>−3</m:mn>
                      </m:mrow>
                    </m:mtd>
                    <m:mtd>
                      <m:mn>1</m:mn>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:mrow>
                        <m:mn>−3</m:mn>
                      </m:mrow>
                    </m:mtd>
                    <m:mtd>
                      <m:mn>1</m:mn>
                    </m:mtd>
                    <m:mtd>
                      <m:mrow>
                        <m:mn>−19</m:mn>
                      </m:mrow>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:mn>1</m:mn>
                    </m:mtd>
                    <m:mtd>
                      <m:mn>0</m:mn>
                    </m:mtd>
                    <m:mtd>
                      <m:mn>5</m:mn>
                    </m:mtd>
                  </m:mtr>

                </m:mtable>
              </m:mrow> <m:mo>]</m:mo></m:mrow>
            </m:mrow>
          </m:math>
          </equation><para id="fs-id1165137727473">Multiply both sides of the equation by <m:math>
            <m:mrow>
              <m:msup>
                <m:mi>A</m:mi>
                <m:mrow>
                  <m:mn>−1</m:mn>
                </m:mrow>
              </m:msup>
              <m:mo>.</m:mo>
            </m:mrow>
          </m:math> We want <m:math>
            <m:mrow>
              <m:msup>
                <m:mi>A</m:mi>
                <m:mrow>
                  <m:mn>−1</m:mn>
                </m:mrow>
              </m:msup>
              <m:mi>A</m:mi><m:mi>X</m:mi><m:mo>=</m:mo><m:msup>
                <m:mi>A</m:mi>
                <m:mrow>
                  <m:mn>−1</m:mn>
                </m:mrow>
              </m:msup>
              <m:mi>B</m:mi><m:mo>:</m:mo>
            </m:mrow>
          </m:math>
          </para>
          <equation id="fs-id1165134043976" class="unnumbered"><label/> <m:math display="block">
 <m:mrow>
  <m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable columnalign="right">
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−2</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−3</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>1</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−3</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>1</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−19</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>1</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>0</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>5</m:mn>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow><m:mspace width="0.8em"/><m:mtext>  </m:mtext><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable columnalign="right">
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>5</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>15</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>56</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mrow>
