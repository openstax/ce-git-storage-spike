      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>,</m:mo><m:mi>B</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable columnalign="right">
    <m:mtr columnalign="right">
