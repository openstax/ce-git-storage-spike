    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>−105</m:mn><m:mn>−26</m:mn><m:mo>+</m:mo><m:mn>133</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>35</m:mn><m:mo>+</m:mo><m:mn>0</m:mn><m:mn>−35</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable>
    <m:mtr>
     <m:mtd>
      <m:mn>1</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mn>2</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mn>0</m:mn>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow>
 </m:mrow>
</m:math>
</equation><para id="fs-id1165135597700">The solution is <m:math>
            <m:mrow>
              <m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mn>1</m:mn><m:mo>,</m:mo><m:mn>2</m:mn><m:mo>,</m:mo><m:mn>0</m:mn>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
            </m:mrow>
          </m:math>
          </para>
        </solution>
      </exercise></example>

