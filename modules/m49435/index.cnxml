         </m:mtd>
         <m:mtd columnalign="right">
          <m:mrow/>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mn>−9</m:mn>
          </m:mrow>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>·</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
