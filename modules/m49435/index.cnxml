                  <m:mspace width="0.8em"/><m:mtable>
                    <m:mtr>
                      <m:mtd>
                        <m:mrow>
                          <m:mn>−2</m:mn>
                        </m:mrow>
                      </m:mtd>
                      <m:mtd>
                        <m:mrow>
                          <m:mn>−3</m:mn>
                        </m:mrow>
                      </m:mtd>
                      <m:mtd>
                        <m:mn>1</m:mn>
                      </m:mtd>
                    </m:mtr>
                    <m:mtr>
                      <m:mtd>
                        <m:mrow>
                          <m:mn>−3</m:mn>
                        </m:mrow>
                      </m:mtd>
                      <m:mtd>
                        <m:mn>1</m:mn>
                      </m:mtd>
                      <m:mtd>
                        <m:mrow>
