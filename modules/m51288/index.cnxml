<para id="fs-id2595586">False</para>
</solution></exercise>
<exercise id="fs-id2595591"><problem id="fs-id2595592"><para id="fs-id2595593"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:msup>
       <m:mrow>
        <m:mi>sec</m:mi>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>−</m:mo><m:msup>
       <m:mrow>
        <m:mi>tan</m:mi>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mi>x</m:mi>
     </m:mrow>
     <m:mrow>
      <m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
     </m:mrow>
    </m:mfrac>

   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>2</m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:mspace width="0.3em"/><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
     </m:mrow>
     <m:mrow>
      <m:mn>2</m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:mspace width="0.3em"/><m:mi>cot</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
     </m:mrow>
    </m:mfrac>

   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>−</m:mo><m:mn>2</m:mn><m:mspace width="0.3em"/><m:msup>
   <m:mrow>
    <m:mi>sin</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>x</m:mi><m:mo>=</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mn>2</m:mn><m:mi>x</m:mi>
 </m:mrow>
</m:math>
</para></problem></exercise>
<exercise id="fs-id1812346"><problem id="fs-id1812347"><para id="fs-id1812348"> <m:math>
