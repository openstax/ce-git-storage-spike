<section id="fs-id2056229" class="key-equations"><title>Key Equations</title><table id="fs-id2056235" summary=".." class="unnumbered unstyled"><label/>
<tgroup cols="2"><colspec colnum="1" colname="c1"/>
<colspec colnum="2" colname="c2"/>
<tbody>
<row>
<entry align="center">Pythagorean identities</entry>
<entry align="center"> <m:math>
<m:mtable columnalign="left">
<m:mtr>
<m:mtd>
<m:msup>
<m:mi>cos</m:mi>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi><m:mo>+</m:mo><m:msup>
<m:mi>sin</m:mi>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mn>1</m:mn><m:mo>+</m:mo><m:msup>
<m:mi>cot</m:mi>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi><m:mo>=</m:mo><m:msup>
<m:mi>csc</m:mi>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mn>1</m:mn><m:mo>+</m:mo><m:msup>
<m:mi>tan</m:mi>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi><m:mo>=</m:mo><m:msup>
<m:mi>sec</m:mi>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi>
</m:mtd>
</m:mtr>
</m:mtable>

</m:math>
</entry>
</row>


<row>
<entry align="center">Even-odd identities</entry>
<entry align="center"> <m:math>
 <m:mrow>
  <m:mtable>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:mi>tan</m:mi><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:mo>−</m:mo><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:mi>cot</m:mi><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:mo>−</m:mo><m:mi>cot</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:mi>sin</m:mi><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:mo>−</m:mo><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:mi>csc</m:mi><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:mo>−</m:mo><m:mi>csc</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mo>=</m:mo>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr>
    <m:mtd>
     <m:mrow>
