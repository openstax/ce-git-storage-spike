<exercise id="fs-id2791132"><problem id="fs-id2791133"><para id="fs-id2791134"> <m:math>
<m:mrow>
<m:mfrac>
<m:mrow>
<m:mn>1</m:mn><m:mo>+</m:mo><m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
</m:mrow>
<m:mrow>
<m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
</m:mrow>
</m:mfrac>
<m:mo>=</m:mo><m:mfrac>
<m:mrow>
<m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
</m:mrow>
<m:mrow>
<m:mn>1</m:mn><m:mo>+</m:mo><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
