</m:mrow>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi>
</m:mrow>
</m:mfrac>
<m:mo>=</m:mo><m:msup>
<m:mrow>
<m:mi>sin</m:mi>
</m:mrow>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi>
</m:mrow>
</m:math>
</para></problem></exercise>
<exercise id="fs-id2427617"><problem id="fs-id2427618"><para id="fs-id2427619"> <m:math>
 <m:mrow>
  <m:mn>3</m:mn><m:mspace width="0.3em"/><m:msup>
   <m:mrow>
    <m:mi>sin</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mspace width="0.3em"/><m:msup>
   <m:mrow>
    <m:mi>cos</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>3</m:mn><m:mo>+</m:mo><m:msup>
   <m:mrow>
    <m:mi>cos</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id2427687">
<para id="fs-id2427689">True <m:math>
 <m:mrow>
  <m:mn>3</m:mn><m:mspace width="0.3em"/><m:msup>
   <m:mrow>
    <m:mi>sin</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mspace width="0.3em"/><m:msup>
   <m:mrow>
    <m:mi>cos</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>3</m:mn><m:mspace width="0.3em"/><m:msup>
   <m:mrow>
    <m:mi>sin</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:mspace width="0.3em"/><m:msup>
   <m:mrow>
    <m:mi>cos</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi><m:mo>+</m:mo><m:msup>
   <m:mrow>
    <m:mi>cos</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>3</m:mn><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:msup>
     <m:mrow>
      <m:mi>sin</m:mi>
     </m:mrow>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>θ</m:mi><m:mo>+</m:mo><m:msup>
     <m:mrow>
      <m:mi>cos</m:mi>
     </m:mrow>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>θ</m:mi>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msup>
   <m:mrow>
    <m:mi>cos</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>3</m:mn><m:mo>+</m:mo><m:msup>
   <m:mrow>
    <m:mi>cos</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>θ</m:mi>
 </m:mrow>
</m:math>

</para></solution></exercise>
<exercise id="fs-id2575546"><problem id="fs-id2575547"><para id="fs-id2575548"> <m:math>
<m:mrow>
<m:mfrac>
<m:mrow>
<m:mi>sec</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>+</m:mo><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
</m:mrow>
<m:mrow>
<m:mi>cot</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>+</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
</m:mrow>
</m:mfrac>
<m:mo>=</m:mo><m:msup>
<m:mrow>
<m:mi>sec</m:mi>
</m:mrow>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi>
</m:mrow>
</m:math>
</para></problem></exercise>

