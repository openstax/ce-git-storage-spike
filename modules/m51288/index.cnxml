<m:mn>1</m:mn><m:mo>+</m:mo><m:msup>
<m:mrow>
<m:mi>sin</m:mi>
</m:mrow>
<m:mn>2</m:mn>
</m:msup>
<m:mi>x</m:mi>
</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:msup>
<m:mrow>
