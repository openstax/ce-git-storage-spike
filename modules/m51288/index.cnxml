<m:mfrac>
<m:mrow>
<m:msup>
<m:mrow>
<m:mi>cos</m:mi>
</m:mrow>
<m:mn>2</m:mn>
</m:msup>
<m:mi>θ</m:mi><m:mo>−</m:mo><m:msup>
