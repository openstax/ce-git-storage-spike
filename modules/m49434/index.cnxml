      <m:mn>2</m:mn>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow>
 </m:mrow>
</m:math>
</para></solution>
</exercise>
<exercise id="fs-id1165137405969"><problem id="fs-id1165137405970"><para id="fs-id1165137405971"> <m:math>
 <m:mrow>
  <m:mtable columnalign="left">
