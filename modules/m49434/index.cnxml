<exercise id="fs-id1165135187927">
<problem id="fs-id1165135187929"><para id="fs-id1165135502718"> <m:math>
 <m:mrow>
  <m:mtable columnalign="left">
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext> </m:mtext><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>8</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>19</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>12</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>4</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>3</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>9</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>−7</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165137418199"><para id="fs-id1165133248573"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mrow><m:mrow>
    <m:mtable columnalign="right">
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mn>1</m:mn>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mrow/>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mn>5</m:mn>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mrow/>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mn>8</m:mn>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mrow/>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mrow>
        <m:mn>12</m:mn>
       </m:mrow>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mrow/>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mn>3</m:mn>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mrow/>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mn>0</m:mn>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mrow/>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mn>3</m:mn>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mrow/>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mn>4</m:mn>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mrow/>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mn>9</m:mn>
      </m:mtd>
      <m:mtd columnalign="right">
       <m:mrow/>
      </m:mtd>
     </m:mtr>

    </m:mtable>
   </m:mrow><m:mo>|</m:mo></m:mrow><m:mtable columnalign="right">
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>16</m:mn>
      </m:mrow>
     </m:mtd>
