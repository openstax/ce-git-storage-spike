      <m:mrow/>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mo>−</m:mo><m:mn>2</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>2</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
