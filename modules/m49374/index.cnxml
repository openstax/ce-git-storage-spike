        <m:mn>1</m:mn>
        <m:mn>2</m:mn>
       </m:mfrac>
       <m:mo>,</m:mo></m:mrow>
     </m:math> find <m:math>
 <m:mrow>
  <m:mi>sec</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>csc</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>,</m:mo><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mo>,</m:mo>
 </m:mrow>
</m:math> and <m:math>
      <m:mrow>
