    </m:msup>

   </m:mrow>
   <m:mn>9</m:mn>
  </m:mfrac>
  <m:mo>−</m:mo><m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>

   </m:mrow>
   <m:mn>1</m:mn>
  </m:mfrac>
  <m:mo>=</m:mo><m:mn>1</m:mn>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1949869"><para id="fs-id1949870"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>3</m:mn><m:msqrt>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>1</m:mn>
   </m:mrow>
  </m:msqrt>
  <m:mo>,</m:mo><m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>3</m:mn><m:msqrt>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>1</m:mn>
   </m:mrow>
  </m:msqrt>

 </m:mrow>
</m:math>
</para>

