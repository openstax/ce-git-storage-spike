   <m:mn>2</m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>6</m:mn>
 </m:mrow>
</m:math> </para></problem></exercise>
<exercise id="fs-id2227261"><problem id="fs-id2227262"><para id="fs-id2227264"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2</m:mn>
    </m:msup>

   </m:mrow>
   <m:mrow>
    <m:mn>36</m:mn>
   </m:mrow>
  </m:mfrac>
  <m:mo>−</m:mo><m:mfrac>
