<row>
<entry>Pythagorean Identities</entry>
<entry> <m:math>
 <m:mtable columnalign="left">
  <m:mtr>
   <m:mtd>
    <m:msup>
     <m:mi>cos</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>θ</m:mi><m:mo>+</m:mo><m:msup>
     <m:mi>sin</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mn>1</m:mn><m:mo>+</m:mo><m:msup>
     <m:mi>tan</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>θ</m:mi><m:mo>=</m:mo><m:msup>
     <m:mi>sec</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>θ</m:mi>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mn>1</m:mn><m:mo>+</m:mo><m:msup>
     <m:mi>cot</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>θ</m:mi><m:mo>=</m:mo><m:msup>
     <m:mi>csc</m:mi>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>θ</m:mi>
   </m:mtd>
  </m:mtr>
 </m:mtable>

</m:math> </entry>
</row>
