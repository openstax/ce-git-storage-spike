    <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mn>4</m:mn><m:mo>−</m:mo><m:mn>2</m:mn><m:mi>i</m:mi>
     </m:mrow>
    <m:mo>)</m:mo></m:mrow>
   </m:mrow>
   <m:mrow>
    <m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo>+</m:mo><m:mi>i</m:mi><m:mo stretchy="false">)</m:mo>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id1591752"><para id="fs-id2703411"> <m:math>
 <m:mrow>
  <m:mn>5</m:mn><m:mn>−5</m:mn><m:mi>i</m:mi>
 </m:mrow>
</m:math> </para></solution></exercise>
