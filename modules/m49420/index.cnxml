     </m:mtable>

    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="0.8em"/><m:mtext/><m:mn>2</m:mn><m:mi>x</m:mi><m:mn>−4</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem></exercise>
