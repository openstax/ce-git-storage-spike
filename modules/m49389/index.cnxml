    <m:mrow>
  <m:mfrac>
      <m:mn>1</m:mn>
      <m:mrow>
       <m:mi>sin</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi></m:mrow>
     </m:mfrac>
     <m:mo>.</m:mo></m:mrow>
   </m:math>
