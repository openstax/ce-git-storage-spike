   <exercise id="fs-id1351072">
    <problem id="fs-id1351074">
     <para id="fs-id1351076"> <m:math>
      <m:mrow>
       <m:mi>cot</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mo>−</m:mo><m:mi>x</m:mi></m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mo>−</m:mo><m:mi>x</m:mi></m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mi>sin</m:mi><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mo>−</m:mo><m:mi>x</m:mi></m:mrow>
          <m:mo>)</m:mo></m:mrow></m:mrow>
     </m:math>
     </para>
    </problem>
   <solution id="fs-id1407071">
    <para id="fs-id1407073"> <m:math>
     <m:mrow>
      <m:mo>−</m:mo><m:mi>cot</m:mi><m:mi>x</m:mi><m:mi>cos</m:mi><m:mi>x</m:mi><m:mo>−</m:mo><m:mi>sin</m:mi><m:mi>x</m:mi></m:mrow>
    </m:math>
    </para>
   </solution>
   </exercise>
