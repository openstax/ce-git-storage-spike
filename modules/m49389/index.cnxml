   <para id="fs-id1365640">For the following exercises, find the period and horizontal shift of each of the functions.</para>
   <exercise id="fs-id1365643">
    <problem id="fs-id1365645">
     <para id="fs-id1562031"> <m:math>
      <m:mrow>
       <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>x</m:mi>
        <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>tan</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>4</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>32</m:mn></m:mrow>
         <m:mo>)</m:mo></m:mrow></m:mrow>
     </m:math>
     </para>
    </problem>
   </exercise>
   <exercise id="fs-id1662387">
    <problem id="fs-id1662389">
     <para id="fs-id1671047"> <m:math>
      <m:mrow>
       <m:mi>h</m:mi><m:mrow><m:mo>(</m:mo>
        <m:mi>x</m:mi>
        <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>sec</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mfrac>
           <m:mi>π</m:mi>
           <m:mn>4</m:mn>
          </m:mfrac>
          <m:mrow><m:mo>(</m:mo>
           <m:mrow>
            <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow>
           <m:mo>)</m:mo></m:mrow></m:mrow>
         <m:mo>)</m:mo></m:mrow></m:mrow>
     </m:math>
     </para>
    </problem>
   <solution id="fs-id1581683">
