<entry align="center"> <m:math><m:mn>-2</m:mn></m:math> </entry>
<entry align="center"> <m:math><m:mn>-1</m:mn></m:math> </entry>
<entry align="center"> <m:math><m:mn>0</m:mn></m:math> </entry>
<entry align="center"> <m:math><m:mn>1</m:mn></m:math> </entry>
<entry align="center"> <m:math><m:mn>2</m:mn></m:math> </entry>
<entry align="center"> <m:math><m:mn>3</m:mn></m:math> </entry>
</row>
<row>
