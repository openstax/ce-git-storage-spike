<m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
<m:mi>x</m:mi>
<m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:msup>
<m:mn>4</m:mn>
<m:mi>x</m:mi>
</m:msup>
<m:mo>−</m:mo><m:mn>3</m:mn>
</m:mrow>
</m:math> </para>
</solution>
</exercise>

<exercise id="fs-id1165137762972">
<problem id="fs-id1165137762974">
<para id="fs-id1165137762976">Shift <m:math>
<m:mrow>
<m:mspace width="0.8em"/><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mspace width="0.8em"/>
</m:mrow>
</m:math> 2 units left</para>
