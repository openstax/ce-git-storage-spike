</exercise>

<exercise id="fs-id1165135437143">
<problem id="fs-id1165135437145">
<para id="fs-id1165135437147">Shift <m:math>
<m:mrow>
<m:mspace width="0.8em"/><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mspace width="0.8em"/>
</m:mrow>
